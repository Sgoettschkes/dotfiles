# File managed by Sgoettschkes/dotfiles
# Do not change

# General

Host *
    Protocol 2
    TCPKeepAlive yes
    ServerAliveInterval 60

# Diagnosia

Host bastion
    HostName bastion.dgns.wtf
    IdentityFile ~/.ssh/bastion.pem
    User ubuntu

## Infrastruktur

Host gitlab
	HostName git.dgns.wtf
	User chuck

Host gitlab-ci-runner-1
    Hostname ip-10-10-1-22.eu-central-1.compute.internal
    IdentityFile ~/.ssh/gitlab-ci-runner.pem
    ProxyJump bastion
    User ubuntu

Host rancher
    HostName 10.10.1.107
    ProxyJump bastion
    User rancher

Host sentry
	HostName sentry.dgns.wtf
	Port 62371
    User chuck
    
