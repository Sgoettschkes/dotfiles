# General

Host *
    Protocol 2
    TCPKeepAlive yes
    ServerAliveInterval 60

# Diagnosia

Host bastion
    HostName bastion.dgns.wtf
    IdentityFile ~/.ssh/bastion.pem
    User ubuntu
    Port 22

