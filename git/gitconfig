# Do not change

[alias]
    # standard
    br = branch
    ci = commit
    co = checkout
    diff = diff --word-diff
    down = pull --rebase
    fe = fetch
    re = reset HEAD
    st = status -s
    up = push origin master

[branch]
	autosetuprebase = always

[color]
    diff = auto
    status = auto
    branch = auto
    ui = auto

[core]
    editor = vim
    excludesfile = ~/.gitignore

[merge]
	conflictstyle = diff3

[push]
    default = upstream

